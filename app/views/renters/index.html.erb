<h1>Renters</h1>
<h3>Hello, <%=current_user.first_name.capitalize%> <%=current_user.last_name.capitalize%>!</h3>
<%= link_to 'New Renter', new_renter_path, class: "btn btn-info" %>

<table class="table table-hover">
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Aka</th>
      <th>Email</th>
      <th>Phone number</th>
      <th>Mobile phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @renters.each do |renter| %>
      <tr>
        <td><%= renter.first_name %></td>
        <td><%= renter.last_name %></td>
        <td><%= renter.aka %></td>
        <td><%= renter.email %></td>
        <td><%= renter.phone_number %></td>
        <td><%= renter.mobile_phone %></td>
        <td><%= link_to 'Show', renter, class: "btn btn-primary" %></td>
        <!-- <td><%#= link_to 'Edit', edit_renter_path(renter), class: "btn btn-link" %></td> -->
        <!-- <td><%#= link_to 'Destroy', renter, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-link" %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>


