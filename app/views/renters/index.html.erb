<p id="notice"><%= notice %></p>

<h1>Renters</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Aka</th>
      <th>Email</th>
      <th>Street address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Phone number</th>
      <th>Mobile phone</th>
      <th>Vrbo url</th>
      <th>Airbnb url</th>
      <th>Facebook url</th>
      <th>Linkedin url</th>
      <th>Misc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @renters.each do |renter| %>
      <tr>
        <td><%= renter.first_name %></td>
        <td><%= renter.last_name %></td>
        <td><%= renter.aka %></td>
        <td><%= renter.email %></td>
        <td><%= renter.street_address %></td>
        <td><%= renter.city %></td>
        <td><%= renter.state %></td>
        <td><%= renter.zip %></td>
        <td><%= renter.phone_number %></td>
        <td><%= renter.mobile_phone %></td>
        <td><%= renter.vrbo_url %></td>
        <td><%= renter.airbnb_url %></td>
        <td><%= renter.facebook_url %></td>
        <td><%= renter.linkedin_url %></td>
        <td><%= renter.misc %></td>
        <td><%= link_to 'Show', renter, class: "btn btn-link" %></td>
        <td><%= link_to 'Edit', edit_renter_path(renter), class: "btn btn-link" %></td>
        <td><%= link_to 'Destroy', renter, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-link" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Renter', new_renter_path, class: "btn btn-link" %>
