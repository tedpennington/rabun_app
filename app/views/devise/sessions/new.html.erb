<div class="jumbotron" id="welcome-jumbo">
    <div id="jumbo-top">
    <h1 class="display-3">Welcome</h1>
    <p class="lead">Rental property landlords and property managers in the Rabun County area!.</p>
    <p>This site is a private online community. To find out more about us or join, click below.</p>
    <p class="lead">
      <!-- <a class="btn btn-primary btn-lg" href="#" role="button">Learn more</a> -->
      <%= link_to  "Learn more", pages_about_path, class: "btn btn-primary btn-lg" %>
    </p>
  </div>
    <hr class="my-4">
    <p class="lead">LOGIN</p>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="form-group row">
      <%= f.label :email, class: "col-2 col-form-label" %><br />
      <%= f.email_field :email, autofocus: true, class: "form-control col-3" %>
    </div>
  
    <div class="form-group row">
      <%= f.label :password, class: "col-2 col-form-label" %><br />
      <%= f.password_field :password, autocomplete: "off", class: "form-control col-3" %>
    </div>
  
    <% if devise_mapping.rememberable? -%>
      <div class="field">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div>
    <% end -%>
  
    <div class="actions">
      <%= f.submit "Log in", class: "btn btn-primary" %>
    </div>
  <% end %>
  
  <%= render "devise/shared/links" %>



</div>


